<!DOCTYPE HTML>
<html>
<head>
	<title><%= nom %> - GÃ©ocaching Jeux de Sophia 2018</title>
	<% include ./partials/head %>
</head>
<body>
	<% include ./partials/menu %>

	<div id="main" class="wrapper style4">

		<div id="content" class="container">
			<section>
				<header class="major">

					<img width="400px;" align="right" src="<%= photo %>" alt="<%= nom %>" style="margin-bottom:20px;">
					<img width="60px;" align="left" src="public/images/tb.jpg" style="margin-right:60px;"/><h2><%= nom %></h2>
					<% if (holder != null) { %>
					<span class="byline">Dans les mains de <%= holder %> <% } %></span>
					
				</header>

				<p><h3>Objet Voyageur de <i><%= owner %></i></h3></p>

				<h3>Description</h3>
				<p><%- description %></p>
				<h3>Mission</h3>
				<p><%- mission %></p>

			</section>
			<br /><br />
			<section>
				<h3>Fil de discussion concernant <%= nom %></h3>
				<br>
				<div id="disqus_thread"></div>
				<script>
				var disqus_config = function () {
					// this.page.url = PAGE_URL; 
					this.page.identifier = <%= id %>;
				};

				(function() {
				var d = document, s = d.createElement('script');
				s.src = 'https://geocaching-jds.disqus.com/embed.js';
				s.setAttribute('data-timestamp', +new Date());
				(d.head || d.body).appendChild(s);
				})();
				</script>
				<noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
			</section>

		</div>
	</div>
	<% include ./partials/footer %>

</body>
</html>
